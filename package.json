    "embla-carousel-svelte": "^8.1.6",
    "firebase": "^10.8.0",
    "firebase-admin": "^12.0.0",
    "formsnap": "^0.4.4",
    "intl-tel-input": "^19.2.19",
    "lucide-svelte": "^0.331.0",
    "mdsvex": "^0.11.0",
    "mode-watcher": "^0.2.1",
    "parse-nested-form-data": "^1.0.0",
    "reveal.js": "^5.0.4",
    "svelte-french-toast": "^1.2.0",
    "sveltekit-superforms": "^2.2.0",
